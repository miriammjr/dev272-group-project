{
  "name": "dev272-group-project",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "dotenv -e .env.local expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "dotenv -e .env.local expo start --android",
    "ios": "dotenv -e .env.local expo start --ios",
    "web": "dotenv -e .env.local expo start --web",
    "lint": "expo lint",
    "prepare": "husky install",
    "test": "jest --watchAll --coverage"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@sentry/react-native|native-base|react-native-svg|@gluestack-ui/.*))"
    ],
    "collectCoverageFrom": [
      "src/**/*.{tsx, ts}"
    ],
    "collectCoverage": true,
    "verbose": true,
    "coverageThreshold": {
      "global": {
        "branches": 20,
        "functions": 20,
        "lines": 20,
        "statements": 20
      }
    }
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "^2.1.2",
    "@react-native-community/datetimepicker": "^8.3.0",
    "@react-native-picker/picker": "^2.11.0",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "@supabase/supabase-js": "^2.50.0",
    "@testing-library/react-native": "^13.2.0",
    "date-fns": "^4.1.0",
    "expo": "^53.0.12",
    "expo-blur": "~14.1.5",
    "expo-constants": "~17.1.6",
    "expo-font": "~13.3.1",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.3.0",
    "expo-linking": "~7.1.5",
    "expo-notifications": "~0.31.3",
    "expo-router": "~5.1.0",
    "expo-splash-screen": "^0.30.9",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "^0.4.5",
    "expo-system-ui": "~5.0.9",
    "expo-web-browser": "~14.2.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "^0.79.4",
    "react-native-animatable": "^1.4.0",
    "react-native-calendars": "^1.1312.1",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "^5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-web": "~0.20.0",
    "react-native-webview": "13.13.5",
    "save-dev": "^0.0.1-security",
    "supabase": "^2.24.3"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.0.10",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.27.0",
    "eslint-config-expo": "~9.2.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-testing-library": "^7.5.3",
    "husky": "^9.1.7",
    "jest": "~29.7.0",
    "jest-environment-jsdom": "^30.0.2",
    "jest-expo": "~53.0.7",
    "lint-staged": "^16.1.0",
    "prettier": "^3.5.3",
    "react-test-renderer": "^19.0.0",
    "typescript": "~5.8.3"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "private": true
}
